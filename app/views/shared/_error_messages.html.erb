<% if object.errors.any? %>
  <div class="error_explaination">
    <div class="alert alert-error">
      The form consists<%= pluralize(object.errors.count, 'error') %>
    </div>
    <ul>
      <% object.errors.full_messages.each do |msg| %>
      <li>* <%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>
